    },
    "devDependencies": {
        "@supercharge/eslint-config-typescript": "~2.3.1",
        "@types/lodash": "~4.14.185",
        "c8": "~7.12.0",
        "eslint": "~8.24.0",
        "expect": "~29.0.3",
        "typescript": "~4.8.3",
        "uvu": "~0.5.6"
    },
    "engines": {
        "node": ">=16"
    },
    "homepage": "https://superchargejs.com",
    "keywords": [
        "config",
